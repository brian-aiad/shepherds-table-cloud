service firebase.storage {
match /b/{bucket}/o {
match /uploads/{orgId}/{allPaths=**} {
allow read: if request.auth != null && exists(/databases/(default)/documents/orgUsers/$(request.auth.uid + '_' + orgId));
allow write: if request.auth != null && exists(/databases/(default)/documents/orgUsers/$(request.auth.uid + '_' + orgId));
}
}
}